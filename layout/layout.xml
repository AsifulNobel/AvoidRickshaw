<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document>
	<mscreen>
		<configuration device="HD" id="config1" name="common"
			orientation="portrait" type="common" />
	</mscreen>
	<views startup="view1">
		<view id="view1" indicator="true" screen_orientation="full_sensor"
			type="view">
			<box homogeneous="false" align_h="0.5" align_v="0.5" visible="true"
				weight_v="1" padding_v="0" id="box1" padding_h="0" weight_h="1"
				direction="vertical">

				<button align_h="0.5" align_v="0.5" visible="true" weight_v="0"
					disable="false" style="circle" id="button1" text="Tap" weight_h="0">
					<event signal="pressed" function_name="view1_button1_onpressed"></event>
				</button>
				<label align_h="0.5" align_v="0.5" visible="true"
					weight_v="0" disable="false" id="label1" text="Latitude Initial: N/A"
					weight_h="0" ellipsis="false"></label>
				<label align_h="0.5" wrap_width="0" align_v="0.5" visible="true"
					weight_v="0" disable="false" line_wrap="none" id="label2" text="Longitude Initial: N/A"
					weight_h="0" ellipsis="false"></label>
				<label align_h="0.5" wrap_width="0" align_v="0.5" visible="true"
					weight_v="0" disable="false" line_wrap="none" id="label3" text="Latitude Final: N/A"
					weight_h="0" ellipsis="false"></label>
				<label align_h="0.5" wrap_width="0" align_v="fill" visible="true"
					weight_v="0" disable="false" line_wrap="none" id="label4" text="Longitude Final: N/A"
					weight_h="0" ellipsis="false"></label>

				<label align_h="0.5" wrap_width="0" align_v="fill" visible="true"
					weight_v="0" disable="false" line_wrap="none" id="labelDistance"
					text="Distance: N/A" weight_h="0" ellipsis="false"></label>

			</box>
		</view>
	</views>
</document>
